{
  "version": "0.0.1",
  "manifest_version": 3,
  "name": "Draft Saver for X(Twitter)",
  "description": "Displays an alert dialog when you try to move a page in X with your post still in progress",
  "content_scripts": [
    {
      "matches": ["https://x.com/*", "https://twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
